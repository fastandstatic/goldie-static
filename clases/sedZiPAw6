<script>jQuery(document).ready(function($) {

        var path = window.location.pathname;
    		var pageName = path === '/' ? 'home' : path.split('/').filter(function(el) { return el.length !== 0; }).pop();
    		console.log("Page name:", pageName);
			
        var pageClass = pageName.replace(/[^a-z0-9\-]/gi, '-').toLowerCase();
        console.log("Page class name:", "page-"+pageClass);

        if (pageClass.length > 0) { 
            $('body').addClass('page-' + pageClass); 
        }

        function updateLogo() {
            if ($('body').hasClass('.home')) {
                $("#logo_header .elementor-widget-container img").attr("src", "/wp-content/uploads/2024/11/Group-59.svg");
            } else {
                if ($(window).scrollTop() > 20) {
                    $("#logo_header .elementor-widget-container img").attr("src", "/wp-content/uploads/2024/11/Logo_volor.svg");
                } else {
                    $("#logo_header .elementor-widget-container img").attr("src", "/wp-content/uploads/2024/11/Group-59.svg");
                }
            }
        }

        function updateStickyHeader() {
            if ($(window).scrollTop() > 20) {
                $("#head").addClass('sticky');
            } else {
                $("#head").removeClass('sticky');
            }
        }
			
        //updateLogo();
       // updateStickyHeader();
			
       // $(window).scroll(function () {
          //updateStickyHeader();
           //updateLogo();
        //});
    });</script>

